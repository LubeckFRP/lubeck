                     , BDPlatform.Pages.Login
                     , BDPlatform.Pages.Campaign
                     , BDPlatform.Pages.Search.Index
                     , BDPlatform.Pages.Search.Instagram
                     , BDPlatform.Pages.Search.Upload
                     , BDPlatform.Pages.Accounts.Index
                     , BDPlatform.Pages.Accounts.Search
                     , BDPlatform.Pages.Accounts.Manage
