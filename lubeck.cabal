                     , BDPlatform.Pages.Results.Index
                     , BDPlatform.Pages.Results.Interactions
                     , BDPlatform.HTMLCombinators
                     , BDPlatform.Validators
                     , BDPlatform.Main
                     , Components.Notifications
                     , Components.BusyIndicator
                     , Components.MainMenu
                     , Components.Map
                     , Components.Grid
                     , Components.Layout
                     , Components.ConfirmDialog
  build-depends:       base
                     , base-prelude
                     , virtual-dom
                     , ghcjs-ffiqq
                     , ghcjs-base
                     , stm
                     , contravariant
                     , vector
                     , profunctors
                     , text
                     , mtl
                     , transformers
                     , time
                     , containers
                     , unordered-containers
                     , lens
                     , semigroups
                     , data-interval
                     , linear
                     , vector-space
                     , colour
                     , xml
                     , neat-interpolation
                     , data-default
                     , containers
                     , aeson
                     , bytestring
                     , monad-parallel
                     , random
                     , ListZipper
                     , split
                     , either
                     , boxes
                     , network-uri
                     , uuid
  hs-source-dirs:      src, lubeck-core/src, lubeck-frp/src, lubeck-drawing/src, lubeck-dv/src
  default-language:    Haskell2010
  cpp-options:         -DGHCJS_BROWSER
  ghc-options:         -fno-warn-typed-holes

